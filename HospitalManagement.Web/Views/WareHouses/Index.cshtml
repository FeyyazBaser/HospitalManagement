@{
    ViewData["Title"] = "Index";
}
@model List<WarehouseWithBuildingDto>
<a class="btn btn-primary" asp-controller="Warehouses" asp-action="Save">Save Warehouse</a>
<table class="table table-striped">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td> 
            <td>Floor</td>
            <td>Building</td>
            <td>CreatedDate</td>
            <td>Actions</td>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Floor</td>
                <td>@item.Building.Name</td>
                <td>@item.CreatedDate</td>
                <td>
                    <a class="btn btn-warning" asp-controller="Warehouses" asp-action="Update" asp-route-id="@item.Id">Update</a>
                    <a class="btn btn-danger" asp-controller="Warehouses" asp-action="Remove" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }

    </tbody>



</table>


